{
  "name": "jaydens-browser",
  "version": "1.0.1",
  "description": "A modern, customizable web browser built with Electron",
  "author": "Jayden",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "build": "electron-builder",
    "build:win": "electron-builder --win"
  },
  "devDependencies": {
    "electron": "^27.0.0",
    "electron-builder": "^26.0.12"
  },
  "build": {
    "appId": "com.jaydensbrowser",
    "productName": "Jaydens' Browser",
    "files": [
      "**/*"
    ],
    "win": {
      "target": [
        "nsis"
      ]
    }
  }
}
